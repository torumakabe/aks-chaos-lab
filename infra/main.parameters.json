{
  "$schema": "https://schema.management.azure.com/schemas/2018-05-01/subscriptionDeploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "${AZURE_LOCATION:japaneast}"
    },
    "appName": {
      "value": "aks-chaos-lab"
    },
    "environment": {
      "value": "${AZURE_ENV_NAME}"
    },
    "nodeVmSize": {
      "value": "Standard_D4ds_v5"
    },
    "vnetAddressPrefix": {
      "value": "10.10.0.0/16"
    },
    "aksSubnetPrefix": {
      "value": "10.10.1.0/24"
    },
    "peSubnetPrefix": {
      "value": "10.10.2.0/24"
    },
    "aksApiSubnetPrefix": {
      "value": "10.10.3.0/28"
    },
    "kubernetesVersion": {
      "value": "1.31"
    },
    "aksSkuName": {
      "value": "Base"
    },
    "enablePrometheusWorkspace": {
      "value": true
    },
    "enablePrometheusPipeline": {
      "value": true
    },
    "enablePrometheusRecordingRules": {
      "value": true
    },
    "enableChaosExperiments": {
      "value": true
    },
    "enableContainerInsights": {
      "value": true
    },
    "actionGroupId": {
      "value": ""
    },
    "chaosNamespace": {
      "value": "chaos-lab"
    },
    "chaosAppLabel": {
      "value": "chaos-app"
    },
    "chaosDuration": {
      "value": "PT5M"
    }
  }
}
